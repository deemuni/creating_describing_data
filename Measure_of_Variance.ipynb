    "# Increase a random sample of 1000 observations from each variable\n",
    "# and store it in a new dataframe.\n",
    "increase_sample=pd.DataFrame()\n",
    "increase_sample['low_var'] = np.random.choice(pop['low_var'], 1000000)\n",
    "increase_sample['high_var']=np.random.choice(pop['high_var'], 1000000)\n",
    "\n",
    "# Again, visualize the data.\n",
    "increase_sample.hist()\n",
    "plt.show()\n",
    "\n",
    "# Check how well the sample replicates the population.\n",
    "increase_sample.mean()\n",
    "increase_sample.std(ddof=1)"
   ]
